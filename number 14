#include<stdio.h>
void main() {
    int a,b;
    int *p1,*p2,r;
    printf("Enter 1st number:");
    scanf("%d",&a);
    printf("Enter 2nd number:");
    scanf("%d",&b);
    printf("Before swaping:a=%d and b=%d\n",a,b);
    p1=&a;
    p2=&b;
    
    r=*p1;
    *p1=*p2;
    *p2=r;
    printf("After swaping:a=%d and b=%d\n",a,b);
}
